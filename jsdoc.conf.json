{
  "plugins": ["plugins/markdown"],
  "opts": {
    "access": "all",
    "encoding": "utf8",
    "template": "node_modules/docdash"
  },
  "templates": {
    "cleverLinks": true,
    "default": {
      "includeDate": false
    }
  },
  "docdash": {
    "static": true,
    "search": true,
    "sectionOrder": [
      "Namespaces",
      "Modules",
      "Classes",
      "Externals",
      "Events",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ]
  }
}
